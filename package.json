{
  "name": "espe-datepicker",
  "version": "0.9.7",
  "description": "ESPE DatePicker for React-Native",
  "source": "index.tsx",
  "main": "index.tsx",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "bump": "standard-version --no-git-tag-version && git push origin && git push origin v$(node -p \"require('./package.json').version\") && echo \"Package $(node -p \"require('./package.json').name\") has been updated to version $(node -p \"require('./package.json').version\")\""
  },
  "keywords": [
    "react-native",
    "datepicker",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/espe-org/espe-datepicker.git"
  },
  "author": "Salavat Iskhakov <salavat@projecto.pro> (https://github.com/espe-org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/espe-org/espe-datepicker/issues"
  },
  "homepage": "https://github.com/espe-org/espe-datepicker#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "19.0.10",
    "react": "19.1.0",
    "react-native": "0.81.2",
    "standard-version": "9.5.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "packageManager": "yarn@3.6.4",
  "dependencies": {
    "espe-actionsheet": "https://github.com/espe-org/espe-actionsheet.git",
    "moment": "2.30.1",
    "react-native-device-info": "15.0.1",
    "react-native-modal": "14.0.0-rc.1",
    "react-native-wheel-pick": "1.2.6"
  }
}
